#include <stdio.h>


float coef(float x0, float y0, float x1, float y1){
    float m;
   m = ((y1-y0)/(x1 - x0));
    
    return m;
}






int main()
{
    float x0,y0,x1,y1;
    printf("Digite as coordenadas iniciais e finais : \n");
    scanf("%f %f %f %f", &x0,&y0,&x1,&y1);
    
    
    printf("%.2f\n", coef(x0,y0,x1,y1));

    return 0;
}
